{
  "name": "@gpr/server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "dbgraph:local": " PGPASSWORD=postgres npx pg-mermaid --dbname gpr_dev --username postgres --host localhost --port 5759",
    "migrate:local": "dotenv -e .env -e local.env -- npx tsx src/initDb.ts && pnpm run dbgraph:local",
    "dev": "pnpm run --silent migrate:local && pnpm run --silent pgtyped && pnpm run --silent dev:fullServer",
    "dev:fullServer": "concurrently -n server,pgtyped,worker1,worker2,cron -c yellow,green,blue,bgYellow,bgGreen 'pnpm run --silent dev:server' 'pnpm run --silent pgtyped:watch' 'pnpm run --silent dev:worker' 'pnpm run --silent dev:worker' 'pnpm run --silent dev:cron'",
    "dev:server": "dotenv -e .env -e local.env -- npx tsx --watch src/main.ts",
    "dev:worker": "dotenv -e .env -e local.env -- npx tsx --watch src/worker.ts",
    "dev:cron": "dotenv -e .env -e local.env -- npx tsx --watch src/cron.ts",
    "pgtyped:watch": "pgtyped -w -c pg-typed.config.json",
    "pgtyped": "pgtyped -c pg-typed.config.json"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@pgtyped/cli": "^2.4.3",
    "@testcontainers/postgresql": "^10.24.2",
    "@types/express": "^5.0.1",
    "@types/express-session": "^1.18.1",
    "@types/node": "^22.14.1",
    "@types/nodemailer": "~6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-github2": "^1.2.9",
    "@types/pg": "^8.11.13",
    "concurrently": "^9.1.2",
    "tsx": "^4.19.3",
    "typescript": "~5.7.3",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "@bull-board/api": "^6.9.2",
    "@bull-board/express": "^6.9.2",
    "@pgtyped/runtime": "^2.4.2",
    "bullmq": "^5.49.2",
    "connect-redis": "^8.0.3",
    "dotenv-cli": "^8.0.0",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "graphql": "^16.10.0",
    "graphql-yoga": "^5.13.4",
    "helmet": "^8.1.0",
    "nodemailer": "^6.10.1",
    "octokit": "4.1.3",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "pg": "^8.15.1",
    "redis": "^4.7.0",
    "zod": "^3.24.3"
  }
}
